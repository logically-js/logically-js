{
    "extends": ["google", "prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": ["prettier", "@typescript-eslint"],
    "rules": {
      "no-invalid-this": 0,
      "prettier/prettier": 2,
      "valid-jsdoc": 0
    }
}
